# Dev box environment variables
# Get BOX_AGENT_SECRET from: SELECT agent_secret FROM box_email_settings WHERE box_id = '<your-box-id>';

BOX_AGENT_SECRET=<get from box_email_settings table>
BOX_API_TOKEN=<same as BOX_AGENT_SECRET>
BOX_API_URL=http://localhost:33000/box
BOX_SUBDOMAIN=<your box subdomain>
BOX_INBOX_DIR=/tmp/box-inbox
BOX_DB_PATH=/tmp/box-agent.db
